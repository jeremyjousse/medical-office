.container
	.row
		.col-md-12
			.panel.panel-default
				.panel-heading
					%h2 Import Patients
				.panel-body
					.div_action.row
						.col-md-4
						.col-md-4.text-center
						.col-md-4.text-right
							= link_to content_tag(:span, "", class: "glyphicon glyphicon-chevron-left") + ' Back', patients_path, :class=>'btn btn-default'
					.col-md-12.text-center
						= form_tag import_patients_path, multipart: true do
							= file_field_tag :file
							= submit_tag "Import"